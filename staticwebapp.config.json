{
  "routes": [
    {
      "route": "/index.html",
      "redirect": "/"
    },
    {
      "route": "/api/*",
      "rewrite": "https://coach-call-fastapi.southeastasia.cloudapp.azure.com/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/",
      "rewrite": "/api/server"
    }
  ],
  "platform": {
    "apiRuntime": "node:20"
  },
  "navigationFallback": {
    "rewrite": "/api/server",
    "exclude": [
      "/api/*",
      "/_nuxt/*",
      "/favicon.*"
    ]
  }
}