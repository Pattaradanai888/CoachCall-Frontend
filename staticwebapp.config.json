{
  "platform": {
    "apiRuntime": "node:20"
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/index.html",
      "redirect": "/"
    },
    {
      "route": "/",
      "rewrite": "/api/server"
    }
  ],
  "navigationFallback": {
    "rewrite": "/api/server"
  }
}